{
  "manifest_version": 2,
  "name": "Masq-chrome-extension",
  "description": "",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://127.0.0.1:8844/src/*","http://127.0.0.1:8080/src/*","http://127.0.0.1:9999/src/*"],
      "js": ["./scripts/bridge.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}